{
    "name": "ostrolucky/traceroute",
    "description": "Detect unused routes",
    "type": "library",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "doctrine/annotations": "^1|^2",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.7|^3",
        "phpunit/phpunit": "^10.5|^11|^12",
        "symfony/browser-kit": "^5.4.45|^6.4.19|^7.0",
        "symfony/dependency-injection": "^5.4.45|^6.4.19|^7.0",
        "symfony/doctrine-bridge": "^5.4.45|^6.4.19|^7.0",
        "symfony/dom-crawler": "^5.4.45|^6.4.19|^7.0",
        "symfony/framework-bundle": "^5.4.45|^6.4.19|^7.0",
        "symfony/phpunit-bridge": "^7.3",
        "symfony/routing": "^5.4.45|^6.4.19|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Ostrolucky\\TraceRoute\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ostrolucky\\TraceRoute\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
