{
    "name": "ostrolucky/traceroute",
    "description": "Detect unused routes",
    "type": "library",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "doctrine/annotations": "^1|^2",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.7|^3",
        "phpunit/phpunit": "^9.3",
        "symfony/browser-kit": "^4.4|^5.0|^6.0",
        "symfony/dependency-injection": "^4.4.3|^5.0.3|^6.0",
        "symfony/framework-bundle": "^4.4.2|^5.0.2|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Ostrolucky\\TraceRoute\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ostrolucky\\TraceRoute\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
